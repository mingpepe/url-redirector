{
  "name": "Microsoft zh-tw to en-us",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "An edge extension to redirect microsoft page from zh-tw to en-us",
  "icons": {
    "16": "icons/icon.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["lib/jquery.min.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab"
  ]
}
